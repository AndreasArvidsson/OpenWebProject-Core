{
    "name": "owp.core",
    "description": "JS core agumentation",
    "version": "2.2.0",
    "files": [
        "src/*"
    ],
    "scripts": {
        "start": "webpack serve --config test/webpack.config.js --mode development --port 9090 --open",
        "perf": "webpack serve --config test/webpack.config.js --mode production --port 9090 --open --env perf",
        "pub": "node node_modules/publish-flatten --flatten src --strip scripts --strip devDependencies"
    },
    "keywords": [
        "core",
        "Open Web Project",
        "owp"
    ],
    "author": "Andreas Arvidsson",
    "license": "MIT",
    "homepage": "https://github.com/AndreasArvidsson/OpenWebProject-Core",
    "repository": {
        "type": "git",
        "url": "https://github.com/AndreasArvidsson/OpenWebProject-Core.git"
    },
    "bugs": {
        "url": "https://github.com/AndreasArvidsson/OpenWebProject-Core/issues"
    },
    "devDependencies": {
        "webpack": "latest",
        "webpack-cli": "latest",
        "webpack-dev-server": "latest",
        "html-webpack-plugin": "latest",
        "babel-loader": "latest",
        "@babel/core": "latest",
        "eslint-loader": "latest",
        "eslint": "latest",
        "babel-eslint": "latest",
        "lodash": "latest",
        "publish-flatten": "latest"
    }
}
